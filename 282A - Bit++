#include <iostream>
#include <string>

using namespace std;
int main(){
    int n;
    cin >> n;
    int x = 0;
    string op;

    while(n--)
    {
        cin >> op;
        if(op == "++X" || op == "X++"){
            x++;
        }else if(op == "--X" || op == "X--"){
            x--;
        }
    }



    cout << x << endl;


    return 0;
}
